{
  "manifest_version": 3,
  "name": "Unblur",
  "description": "Helps you unblur everything on the page",
  "version": "1.0",
  "action": {
    "default_title": "Unblur all content"
  },
  "web_accessible_resources": [
    {
      "resources": ["/assets/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },

  "permissions": ["activeTab"]
}
